<div>
	<div class="table-responsive">
		<table class="table table-condensed">
			<thead>
				<tr>
					<th>Id</th>
					<th>First Name</th>
					<th>Last Name</th>
					<th></th>
				</tr>
			</thead>
			<tbody>
				{{#each data}}
					<tr>
						<td>{{this.id}}</td>
						<td>{{this.firstName}}</td>
						<td>{{this.lastName}}</td>
						</td>
						<td>
							<!-- <a href="/employee/edit/{{this.id}}">Edit</a> -->
							<input type="button" value="Edit" onclick="editBtn({{this.id}})">
							<form action="/employee/delete/{{this.id}}" method="post">
								<input type="submit" value="Delete">
							</form>
						</td>
					</tr>
				{{/each}}
			</tbody>
			<tfooter>
				<tr><td>
				<button class="btn btn-primary"
						onClick="addCourse();">Add</button>
				</td></tr>
			</tfooter>
		</table>
	</div>
</div>
